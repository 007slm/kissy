<project name="selectable2.build" default="build" basedir=".">
    <description>Component Build File</description>

    <import file="../../tools/build.xml"/>

    <property file="build.properties"/>
    <property name="src.dir" location="."/>

    <target name="concat">
        <concat destfile="selectable2-pkg.js" encoding="${charset}" outputencoding="${charset}">
            <fileset includes="*.js" dir="."/>
            <path path="../selectable2.js"/>
        </concat>
    </target>

    <target name="clean">
        <delete file="selectable2-pkg.js"/>
    </target>

    <target name="build" depends="concat,common.build,clean">
    </target>
</project>
